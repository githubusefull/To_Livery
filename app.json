{
  "expo": {
    "name": "To_Livery",
    "slug": "To_Livery",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/car.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/car2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/car8.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.simomobile.To_Livery"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "cc8c7de7-9b4c-44cb-ba59-f4fd5d3302be"
      }
    },

    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ]
  }
}
